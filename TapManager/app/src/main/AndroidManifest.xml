<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.t_gamer.tapmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.LoginCadastroActivity"
            android:label="@string/title_activity_login_cadastro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.CadastroActivity" />
        <activity
            android:name=".Activity.Home1Activity"
            android:label="@string/title_activity_home1"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>